#SERVER
PORT = 3333

#VARIAVEIS DO TYPEORM

TYPEORM_NAME = 'default'
TYPEORM_SYNCHRONIZE = true
TYPEORM_LOGGING = true
TYPEORM_SSL = false
TYPEORM_REJECT_UNAUTHORIZED = false
rejectUnauthorized
TYPEORM_ENTITIES = ./src/models/*.ts
TYPEORM_MIGRATIONS = ./src/database/migrations/*.ts
TYPEORM_MIGRATIONS_DIR = ./src/database/migrations

#VARIAVEIS DO BANCO DE DADOS - POSTGRES
DATABASE_URL = postgres://postgres:admin@localhost:5432/topic

UUID_EXTENSION = uuid-ossp
TYPE_CONNECTION = postgres
DB_HOST = localhost
DB_USERNAME = postgres
DB_PASSWORD = admin
DB_PORT = 5432
NAME_DATABASE = topic
